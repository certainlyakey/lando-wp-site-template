# ignore OS generated files
ehthumbs.db
Thumbs.db
.DS_Store

# ignore environment data
.env

# ignore server configs
.htaccess

# ignore some accidental stuff that might make it into the repo
node_modules
/backups
/sql-dumps/*.gz
/sql-dumps/*.sql

# ignore editor configs
.vscode
*.sublime-project
*.sublime-workspace

# ignore Wordpress core
/wp

# ignore Wordress content (exceptions below)
/wp-content/*
!/wp-content/mu-plugins/
!/wp-content/plugins/
!/wp-content/themes/

# ignore all mu-plugins, plugins, and themes
# unless explicitly whitelisted at the end of this file
/wp-content/mu-plugins/*
/wp-content/plugins/*
/wp-content/themes/*

# ignore development softlinks
/assets
!/assets/

# Exceptions
# !/wp-content/themes/lando-wp-site-theme
